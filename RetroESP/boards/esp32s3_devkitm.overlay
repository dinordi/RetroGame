
&spi2 {
        status = "okay";
        clock-frequency = <10000000>;
        cs-gpios = <&gpio0 10 GPIO_ACTIVE_LOW>;

};

/ {
	zephyr,user {
		io-channels =
			<&adc0 0>;
			// <&adc1 0>;
	};
	aliases {
		adc0 = &adc0;
		// adc1 = &adc1;
		pin = &button1;
	};
	buttons {
		button1: button_1 {
			gpios = <&gpio1 14 0x11>;
			label = "Button 1";
			zephyr,code = <0xb>;
		};
	};
	
};
